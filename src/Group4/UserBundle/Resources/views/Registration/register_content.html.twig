{% trans_default_domain 'FOSUserBundle' %}

<form role="form" class="well col-lg-5" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <input type="submit" class="btn btn-default" value="{{ 'registration.submit'|trans }}" />
    </div>
</form>
